<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.selectall">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name="com.example.administrator.address.AddressActivity" />
        <activity android:name="com.example.administrator.updateuserinfo.UpdateUserInfoActivity" />
        <activity android:name="com.example.administrator.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.swipemenu.LinearLayoutDelDemoActivity" />
        <activity android:name="com.example.administrator.swipemenu.SwipeMenuDeleteActivity" />
        <activity android:name="com.example.administrator.groupedrecyclerview.StickyActivity" />
        <activity android:name="com.example.administrator.payui.PayActivity" />
        <activity android:name="com.example.administrator.postimg.PostImgActivity" />
        <activity android:name="com.example.administrator.okhttp.OkHttpActivity"></activity>
    </application>

</manifest>